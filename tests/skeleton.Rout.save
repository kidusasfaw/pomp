> pomp.skeleton <- function(times,y,p,more) {
+ # Turns a skeleton function from a 'pomp' object into the right hand
+ # side of and ODE for use in CollocInfer
+   skeleton(more$pomp.obj,x=y,params=p,t=times)
+ }
